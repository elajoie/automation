{% for d in doorsensor %}
  - platform: mqtt
    name: "{{ d.name }}"
    state_topic: "stat/{{ d.name }}/BATT"
    icon: mdi:mdi-battery
    retain: true
    unique_id: '{{ d.name }}_battery'
    device:
      identifiers:
        - '{{ d.name }}'
      name: "{{ d.name }}"
      manufacturer: Tasmota

{% endfor %}